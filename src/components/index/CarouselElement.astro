---
export interface Props {
    imgName: string;
}

const { imgName } = Astro.props;
---

<div class="element-container">
    <img src={imgName} alt="">
</div>

<style>
    .element-container {
        height: 100vh;
        width: 410px;
        background-color: antiquewhite;
        margin-right: 20px;
    }
    .element-container img {
        width: 100%;
        height: 100%;
    }
</style>