---
import ProyectItem from "./ProyectCard.astro";

type dataImg = {
  url: string;
  title: string;
  text: string;
};

export interface Props {
  data: Array<dataImg>;
}

const { data } = Astro.props;
---

{
  data.map((item) => (
    <ProyectItem url={item.url} title={item.title} text={item.text} />
  ))
}
